<% content_for :subheader do %>
<section id="subheader">
  <div class="container">
    <div class='row-fluid'>
      <div class='span12'>
        <h1 class='subheader-title'>Startups a collection of internet companies made with <i class='icon-heart-1 loved'></i> in PR</h1>
      </div>
    </div>
  </div>
</section>
<% end %>
<section id='content'>
  <div class='container'>
    <div class='row-fluid'>
      <% @startups.each do |startup| %>
        <%= render 'startup_card', startup: startup %>
      <% end %>
    </div>
  </div>
</section>

